# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_QUEUE=cache_updates
RABBITMQ_EXCHANGE=cache_exchange
RABBITMQ_ROUTING_KEY=cache.update

# Dead Letter Queue Configuration (optional - defaults will be created if not specified)
RABBITMQ_DLQ_QUEUE=cache_updates_dlq
RABBITMQ_DLQ_EXCHANGE=cache_exchange_dlq
RABBITMQ_DLQ_ROUTING_KEY=cache.update.dlq

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Logging
LOG_LEVEL=info